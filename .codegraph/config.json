    "**/.dart_tool/**",
    "**/.vs/**",
    "**/.nuget/**",
    "**/artifacts/**",
    "**/publish/**",
    "**/cmake-build-*/**",
    "**/CMakeFiles/**",
    "**/bazel-*/**",
    "**/vcpkg_installed/**",
    "**/.conan/**",
    "**/Debug/**",
    "**/Release/**",
    "**/x64/**",
    "**/release/**",
    "**/*.app/**",
    "**/*.asar",
    "**/DerivedData/**",
    "**/.build/**",
    "**/.swiftpm/**",
    "**/xcuserdata/**",
    "**/Carthage/Build/**",
    "**/SourcePackages/**",
    "**/.composer/**",
    "**/storage/framework/**",
    "**/bootstrap/cache/**",
    "**/.bundle/**",
    "**/tmp/cache/**",
    "**/public/assets/**",
    "**/public/packs/**",
    "**/.yardoc/**",
    "**/coverage/**",
    "**/htmlcov/**",
    "**/.nyc_output/**",
    "**/test-results/**",
    "**/.coverage/**",
    "**/.idea/**",
    "**/logs/**",
    "**/tmp/**",
    "**/temp/**",
    "**/_build/**",
    "**/docs/_build/**",
    "**/site/**"
  ],
  "languages": [],
  "frameworks": [],
  "maxFileSize": 1048576,
  "extractDocstrings": true,
  "trackCallSites": true,
  "enableEmbeddings": false
