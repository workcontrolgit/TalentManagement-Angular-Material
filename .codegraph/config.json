    "**/.nx/**",
    "**/.yarn/cache/**",
    "**/.pnpm-store/**",
    "**/storybook-static/**",
    "**/.expo/**",
    "**/web-build/**",
    "**/ios/Pods/**",
    "**/ios/build/**",
    "**/android/build/**",
    "**/android/.gradle/**",
    "**/__pycache__/**",
    "**/.venv/**",
    "**/venv/**",
    "**/site-packages/**",
    "**/dist-packages/**",
    "**/.pytest_cache/**",
    "**/.mypy_cache/**",
    "**/.ruff_cache/**",
    "**/.tox/**",
    "**/.nox/**",
    "**/*.egg-info/**",
    "**/.eggs/**",
    "**/go/pkg/mod/**",
    "**/target/debug/**",
